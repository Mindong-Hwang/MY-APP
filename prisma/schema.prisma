datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Clothing {
  id          Int      @id @default(autoincrement())
  name        String
  minTemp     Int
  maxTemp     Int
  weather     String   // 쉼표로 구분된 문자열로 변경
  image       String
  description String
}
